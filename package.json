{
  "name": "vilog",
  "version": "1.0.0",
  "description": "An interactive visitor management system",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "client": "npm start --prefix frontend",
    "server": "nodemon backend/server.js",
    "dev": "concurrently \"npm run client\" \"npm run server\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theophilusgordon/amalitech-vilog.git"
  },
  "keywords": [
    "vilog",
    "amalitech",
    "visitor-management"
  ],
  "author": "Theophilus Gordon",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/theophilusgordon/amalitech-vilog/issues"
  },
  "homepage": "https://github.com/theophilusgordon/amalitech-vilog#readme",
  "dependencies": {
    "aws-sdk": "^2.1201.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-handler": "^1.2.0",
    "fast-csv": "^4.3.6",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.7.8",
    "pg": "^8.7.3",
    "qrcode": "^1.5.1",
    "sms-service": "^1.1.7",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.5.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "concurrently": "^7.3.0",
    "nodemon": "^2.0.19"
  }
}
